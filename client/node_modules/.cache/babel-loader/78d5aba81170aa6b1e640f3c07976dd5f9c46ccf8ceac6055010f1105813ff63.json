{\r\n            try {\r\n                const res=await axios.get('news'+this.id)\r\n                \r\n            } catch (err) {\r\n                alert('Loi!')\r\n            }\r\n        }\r\n    }\r\n}\r\n</script>"],"mappings":"AA+CA,OAAAA,KAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,EAAA,OAAAC,MAAA,CAAAC,KAAA,CAAAF;IACA;EACA;EACAG,QAAA,GAEA;EACAC,OAAA;IACA,MAAAC,SAAA;MACA;QACA,MAAAC,GAAA,SAAAT,KAAA,CAAAU,GAAA,eAAAP,EAAA;MAEA,SAAAQ,GAAA;QACAC,KAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}