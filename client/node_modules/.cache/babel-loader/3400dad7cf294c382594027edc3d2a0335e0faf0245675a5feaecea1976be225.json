{"ast":null,"code":"import { Quill } from 'vue-quill-editor';\nimport ImageResize from 'quill-image-resize-module';\nimport ImageDrop from 'quill-image-drop-module';\nimport ImageExtend from 'quill-image-extend-module';\nQuill.register('modules/imageResize', ImageResize);\nQuill.register('modules/imageDrop', ImageDrop);\nQuill.register('modules/imageExtend', ImageExtend);\nexport default {\n  name: 'NewView',\n  data() {\n    return {\n      content: '',\n      options: {\n        modules: {\n          imageResize: {\n            displaySize: true\n          },\n          imageDrop: true,\n          imageExtend: {\n            loadingClass: 'loading',\n            progressBar: true,\n            deleteButton: true,\n            deleteMessage: 'Are you sure to delete this image?',\n            uploadUrl: '/upload',\n            uploadMethod: 'POST',\n            uploadHeaders: {\n              'Authorization': `Bearer ${yourAccessToken}`\n            },\n            callbackOK: (serverResponse, next) => {\n              next(serverResponse.url);\n            },\n            callbackKO: serverError => {\n              console.error(serverError);\n            }\n          }\n        }\n      }\n    };\n  }\n};","map":{"version":3,"names":["Quill","ImageResize","ImageDrop","ImageExtend","register","name","data","content","options","modules","imageResize","displaySize","imageDrop","imageExtend","loadingClass","progressBar","deleteButton","deleteMessage","uploadUrl","uploadMethod","uploadHeaders","yourAccessToken","callbackOK","serverResponse","next","url","callbackKO","serverError","console","error"],"sources":["src/components/admin/New/NewView.vue"],"sourcesContent":["<template>\r\n    <div class=\"min-h-[60vh] mt-2 relative\">\r\n        <div class=\"flex justify-between px-4\">\r\n            <h1 class=\"font-bold uppercase\">Tạo bài viết</h1>\r\n        </div>\r\n        <div class=\"bg-white shadow-lg mt-1 h-[55vh]\">\r\n            <quill-editor\r\n            clas