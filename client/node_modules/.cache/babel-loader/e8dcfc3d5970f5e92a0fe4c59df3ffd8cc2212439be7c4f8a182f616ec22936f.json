{"ast":null,"code":"import axios from 'axios';\nimport PieChartVue from '../piechart/PieChart.vue';\nimport LineChart from '../splinechart/SplineChart.vue';\nexport default {\n  name: 'ChartView',\n  components: {\n    LineChart,\n    PieChartVue\n  },\n  data() {\n    return {\n      data: [],\n      categories: [],\n      revenueByMonth: []\n    };\n  },\n  methods: {\n    async getTotalByMonth() {\n      try {\n        const res = await axios.get('order/totalByMonth');\n        this.data = res.data;\n      } catch (err) {\n        alert(\"Loi!\");\n      }\n    }\n  },\n  async mounted() {\n    await this.getTotalByMonth();\n    const categories = Array.from({\n      length: 12\n    }, (v, i) => `${i + 1}`);\n    this.categories = categories;\n    const revenueByMonth = new Array(12).fill(0);\n    this.data.map(item => {\n      if (item._id && item.totalRevenue) {\n        revenueByMonth[item._id - 1] = item.totalRevenue / 1000000;\n        this.revenueByMonth = revenueByMonth;\n      }\n    });\n    console.log(this.categories, this.revenueByMonth);\n  }\n};","map":{"version":3,"names":["axios","PieChartVue","LineChart","name","components","data","categories","revenueByMonth","methods","getTotalByMonth","res","get","err","alert","mounted","Array","from","length","v","i","fill","map","item","_id","totalRevenue","console","log"],"sources":["src/components/chart/ChartView.vue"],"sourcesContent":["<template>\r\n     <div class=\"grid grid-cols-5 gap-2\">\r\n        <div class=\" col-span-3 shadow-lg\">\r\n            <LineChart :categories=\"categories\" :revenueByMonth=\"revenueByMonth\" />\r\n        </div>\r\n        <div class=\" col-span-2 shadow-lg  \">\r\n           <div class=\"\"> <PieChartVue :categories=\"categories\" :revenueByMonth=\"revenueByMonth\" /></div>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\n\r\nimport axios from 'axios'\r\nimport PieChartVue from '../piechart/PieChart.vue'\r\nimport LineChart from '../splinechart/SplineChart.vue'\r\nexport default {\r\n    name:'ChartView',\r\n    components:{LineChart,PieChartVue},\r\n    data(){\r\n        return{\r\n            data:[],\r\n            categories:[],\r\n            revenueByMonth:[]\r\n        }\r\n    },\r\n    methods:{\r\n        async getTotalByMonth(){\r\n            try {\r\n                const res=await axios.get('order/totalByMonth')\r\n                this.data=res.data\r\n            } catch (err) {\r\n                alert(\"Loi!\")\r\n            }\r\n        }\r\n    },\r\n    async mounted(){\r\n        await this.getTotalByMonth()\r\n            const categories = Array.from({ length: 12 }, (v, i) => `${i + 1}`);\r\n            this.categories=categories\r\n            const revenueByMonth= new Array(12).fill(0);\r\n            this.data.map(item => {\r\n            if (item._id && item.totalRevenue) {\r\n              revenueByMonth[item._id - 1] = item.totalRevenue /1000000;\r\n              this.revenueByMonth=revenueByMonth\r\n            }\r\n          });\r\n        \r\n        console.log(this.categories,this.revenueByMonth)\r\n    }\r\n}\r\n</script>"],"mappings":"AAYA,OAAAA,KAAA;AACA,OAAAC,WAAA;AACA,OAAAC,SAAA;AACA;EACAC,IAAA;EACAC,UAAA;IAAAF,SAAA;IAAAD;EAAA;EACAI,KAAA;IACA;MACAA,IAAA;MACAC,UAAA;MACAC,cAAA;IACA;EACA;EACAC,OAAA;IACA,MAAAC,gBAAA;MACA;QACA,MAAAC,GAAA,SAAAV,KAAA,CAAAW,GAAA;QACA,KAAAN,IAAA,GAAAK,GAAA,CAAAL,IAAA;MACA,SAAAO,GAAA;QACAC,KAAA;MACA;IACA;EACA;EACA,MAAAC,QAAA;IACA,WAAAL,eAAA;IACA,MAAAH,UAAA,GAAAS,KAAA,CAAAC,IAAA;MAAAC,MAAA;IAAA,IAAAC,CAAA,EAAAC,CAAA,QAAAA,CAAA;IACA,KAAAb,UAAA,GAAAA,UAAA;IACA,MAAAC,cAAA,OAAAQ,KAAA,KAAAK,IAAA;IACA,KAAAf,IAAA,CAAAgB,GAAA,CAAAC,IAAA;MACA,IAAAA,IAAA,CAAAC,GAAA,IAAAD,IAAA,CAAAE,YAAA;QACAjB,cAAA,CAAAe,IAAA,CAAAC,GAAA,QAAAD,IAAA,CAAAE,YAAA;QACA,KAAAjB,cAAA,GAAAA,cAAA;MACA;IACA;IAEAkB,OAAA,CAAAC,GAAA,MAAApB,UAAA,OAAAC,cAAA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}