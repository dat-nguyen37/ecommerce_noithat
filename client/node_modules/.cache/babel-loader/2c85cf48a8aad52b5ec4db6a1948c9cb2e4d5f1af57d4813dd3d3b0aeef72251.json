            selling.classList.add('hidden')\r\n            news.classList.remove('hidden')\r\n            hots.classList.remove('block')\r\n            selling.classList.remove('block')\r\n        })\r\n        sellingTab.addEventListener(\"click\",function(){\r\n            self.getSell()\r\n            hotTab.classList.remove('text-black')\r\n            newTab.classList.remove('text-black')\r\n            sellingTab.classList.add('text-black')\r\n\r\n            news.classList.add('hidden')\r\n            hots.classList.add('hidden')\r\n            selling.classList.add('block')\r\n            news.classList.remove('block')\r\n            hots.classList.remove('block')\r\n            selling.classList.remove('hidden')\r\n        })\r\n    },\r\n\r\n}\r\n</script>"],"mappings":"AAuBA,OAAAA,KAAA;AACA,OAAAC,WAAA;AACA;EACAC,IAAA;EACAC,UAAA;IAAAF;EAAA;EACAG,KAAA;IACA;MACAA,IAAA;IACA;EACA;EACAC,OAAA;IACA,MAAAC,QAAA;MACA;QACA,MAAAC,GAAA,SAAAP,KAAA,CAAAQ,GAAA;QACA,KAAAJ,IAAA,GAAAG,GAAA,CAAAH,IAAA;MACA,SAAAK,GAAA;QACAC,KAAA;MACA;IACA;IACA,MAAAC,OAAA;MACA;QACA,MAAAJ,GAAA,SAAAP,KAAA,CAAAQ,GAAA;QACA,KAAAJ,IAAA,GAAAG,GAAA,CAAAH,IAAA;MACA,SAAAK,GAAA;QACAC,KAAA;MACA;IACA;EACA;EACAE,QAAA;IACA,MAAAC,IAAA;IACA,MAAAC,MAAA,GAAAC,QAAA,CAAAC,cAAA;IACA,MAAAC,MAAA,GAAAF,QAAA,CAAAC,cAAA;IACA,MAAAE,UAAA,GAAAH,QAAA,CAAAC,cAAA;IAEA,MAAAG,IAAA,GAAAJ,QAAA,CAAAC,cAAA;IACA,MAAAI,IAAA,GAAAL,QAAA,CAAAC,cAAA;IACA,MAAAK,OAAA,GAAAN,QAAA,CAAAC,cAAA;IAGAF,MAAA,CAAAQ,gBAAA;MACAR,MAAA,CAAAS,SAAA,CAAAC,GAAA;MACAP,MAAA,CAAAM,SAAA,CAAAE,MAAA;MACAP,UAAA,CAAAK,SAAA,CAAAE,MAAA;MAEAN,IAAA,CAAAI,SAAA,CAAAC,GAAA;MACAJ,IAAA,CAAAG,SAAA,CAAAC,GAAA;MACAH,OAAA,CAAAE,SAAA,CAAAC,GAAA;MACAL,IAAA,CAAAI,SAAA,CAAAE,MAAA;MACAL,IAAA,CAAAG,SAAA,CAAAE,MAAA;MACAJ,OAAA,CAAAE,SAAA,CAAAE,MAAA;IACA;IACAR,MAAA,CAAAK,gBAAA;MACAR,MAAA,CAAAS,SAAA,CAAAE,MAAA;MACAR,MAAA,CAAAM,SAAA,CAAAC,GAAA;MACAN,UAAA,CAAAK,SAAA,CAAAE,MAAA;MAEAN,IAAA,CAAAI,SAAA,CAAAC,GAAA;MACAJ,IAAA,CAAAG,SAAA,CAAAC,GAAA;MACAH,OAAA,CAAAE,SAAA,CAAAC,GAAA;MACAL,IAAA,CAAAI